{
  "extends": ["airbnb", "plugin:import/react"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "document": false
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js",".jsx"]
      }
    }
  }
}
