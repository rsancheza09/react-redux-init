{
  "name": "CloudCover",
  "version": "1.0.0",
  "description": "This is a single app application for hadnling the clouds into a picture.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "prod": "npm run prestart && NODE_ENV=production npm run build",
    "dev": "webpack-dev-server --watch --hot --open",
    "prestart": "npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage-all": "jest --coverage --watchAll",
    "lint": "eslint ./app/src/js/**.js*"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/app/src/js/*.{js,jsx}"
    ]
  },
  "author": "Digital Globe - Barva Team",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^1.0.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.10.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9"
  },
  "dependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^2.9.1",
    "esri-leaflet": "^2.0.8",
    "extract-text-webpack-plugin": "^3.0.0",
    "leaflet": "^1.1.0",
    "leaflet-draw": "^0.4.10",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.5"
  }
}
